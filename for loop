##for loop---only works for collections i.e. list, tuple, dict etc.
li=[1,2,3,4]
for i in li:
    print(i)

              or

for i in range(1,11):
    print(i)


#breaking
for i in range(1,11):
    if i==5:
        break
    print(i)

#skipping
for i in range(1,11):
    if i==3:
        continue
    print(i)

#nested for loop
li=['python','kamna']
for i in li:
    for j in i:
        if j=='t':
            break
        print(j)
